<assembly>
  <id>bin</id>
  <formats>
    <format>zip</format>
  </formats>
  <dependencySets>
    <dependencySet>
      <useProjectArtifact>true</useProjectArtifact>
      <outputDirectory>lib</outputDirectory>
    </dependencySet>
  </dependencySets>
  <fileSets>
    <fileSet>
      <outputDirectory></outputDirectory>
      <includes>
        <include>README.md</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>${project.build.directory}${file.separator}bin</directory>
      <outputDirectory>${file.separator}bin</outputDirectory>
      <includes>
        <include>*.sh</include>
        <include>*.bat</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>${project.build.directory}${file.separator}conf</directory>
      <outputDirectory>${file.separator}conf</outputDirectory>
      <includes>
        <include>*</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
